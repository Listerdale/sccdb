/* components/headerElite/headerElite.wxss */
/*标题行整体框架设置*/
.flex-container{
  margin: auto;
  width: 700rpx;
  display: flex;
  justify-content: flex-start;
  flex-wrap: nowrap;
}
/*排名列标签*/
.headerRank{
  width: 100rpx;
  height: 80rpx;
  border: 1px solid;
  text-align: center;
  font-weight: bold;
  background-color: #06363C;
  color: #F6F6F6;
  line-height: 80rpx;
}
/*号码列标签*/
