/* pages/Integral/index.wxss */
/*整体框架设置*/
.flex-container{
  margin: auto;
  width: 700rpx;
  display: flex;
  justify-content: flex-start;
  flex-wrap: nowrap;
}
/*排名列标题*/
.headerRank{
  width: 150rpx;
  height: 80rpx;
  border: 1px solid;
  text-align: center;
  font-weight: bold;
  background-color: #06363C;
  color: #F6F6F6;
  line-height: 80rpx;
}
/*姓名列标题*/
.headerName{
  width: 350rpx;
  height: 80rpx;
  border: 1px solid;
  text-align: center;
  font-weight: bold;
  background-color: #06363C;
  color: #F6F6F6;
  line-height: 80rpx;
}
/*积分列标题*/
.headerIntegral{
  width: 200rpx;
  height: 80rpx;
  border: 1px solid;
  text-align: center;
  font-weight: bold;
  background-color: #06363C;
  color: #F6F6F6;
  line-height: 80rpx;
}