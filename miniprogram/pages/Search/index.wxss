/* pages/Search/index.wxss */
.search{
  display: flex;
  width: 700rpx;
  height: 40rpx;
  background-color: #f6f6f6ab;
  border: 20rpx solid #f6f6f6ab;
  border-left: 25rpx solid #f6f6f6ab;
  border-right: 25rpx solid #f6f6f6ab;
}
.search input{
  width: 500rpx;
  background-color: white;
}
.search .s{
  width: 200rpx;
  text-align: center;
}
.flex{
  display: flex;
  width: 750rpx;
  justify-content: space-between;
  border-bottom: solid grey;
  border-width: thin;
}
.sItem{
  width: 500rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-indent: 50rpx;
}
.label{
  width: 150rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: right;
}
.flex image{
  width: 50rpx;
  height: 50rpx;
  margin-top: 25rpx;
}
.bl{
  width: 50rpx;
}